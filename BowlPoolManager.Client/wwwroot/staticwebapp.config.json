{
    "navigationFallback": {
        "rewrite": "/index.html"
    },
    "routes": [
        {
            "route": "/admin/*",
            "allowedRoles": [
                "administrator"
            ]
        },
        {
            "route": "/login",
            "rewrite": "/.auth/login/google"
        },
        {
            "route": "/logout",
            "rewrite": "/.auth/logout"
        }
    ],
    "responseOverrides": {
        "401": {
            "redirect": "/.auth/login/google?post_login_redirect_uri=/"
        }
    }
}